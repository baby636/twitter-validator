{
  "name": "external-initiator",
  "version": "0.0.1",
  "description": "Chainlink External Initiator",
  "license": "MIT",
  "engines": {
    "node": "12.18.0"
  },
  "scripts": {
    "build": "tsc -p . --outDir ./dist",
    "dev": "dotenv -e test.env -- ts-node-dev -T index.ts",
    "test": "dotenv -e test.env -- jest --config jest.config.js --logHeapUsage",
    "generate-apidoc": "apidoc -i controllers -o ./apidoc && apidoc-markdown -p ./apidoc -o ./API.md && rm -rf ./apidoc"
  },
  "main": "index.ts",
  "dependencies": {
    "body-parser": "^1.19.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "morgan-body": "^2.4.14",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "0.8.1",
    "typed-rest-client": "^1.7.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.155",
    "@types/node": "12",
    "@types/supertest": "^2.0.9",
    "apidoc": "^0.23.0",
    "apidoc-markdown": "^7.2.0",
    "dotenv-cli": "^3.1.0",
    "jest": "^26.0.1",
    "nock": "^12.0.3",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "~3.9.3"
  }
}
